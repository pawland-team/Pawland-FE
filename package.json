{
  "name": "pawland-fe",
  "version": "0.1.0",
  "private": true,
  "description": "This is the frontend code for Pawland",
  "engines": {
    "node": "20.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pawland-team/Pawland-FE"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint --fix \"**/*.{css,ts,tsx}\"",
    "lint:fix": "prettier --write \"**/*.{ts,tsx}\" && eslint --fix \"**/*.{ts,tsx}\" & stylelint --fix \"**/*.{css,ts,tsx}\"",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,ts,tsx}": [
      "stylelint --fix"
    ]
  },
